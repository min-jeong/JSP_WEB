<?xml version="1.0" encoding="UTF-8"?>
<query> <!--  루트노드 -->
	<movie>
		<getAllMovie> <!-- 리프노드 -->
			SELECT   M.*
         			 , G.GRADE_TITLE
         			 , AC.ACTOR_COUNT
			FROM     MOVIE M
         			 , GRADE G
        			 , (
				            SELECT  MOVIE_ID
				            	    , COUNT(ACTOR_LIST_ID) ACTOR_COUNT
				            FROM    ACTOR_LIST
				            GROUP   BY MOVIE_ID
         			    ) AC
			WHERE    M.GRADE_ID = G.GRADE_ID
			AND      AC.MOVIE_ID(+) = M.MOVIE_ID 
		</getAllMovie>
	</movie>
</query>